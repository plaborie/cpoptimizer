
SLIB    = ../../lib$L.a
ARFLAGS = cr

o      = .o

.c.o:
	$(CC) $(CFLAGS) $(FLAGS) -I../../incl $(IPATH) $(DFLAGS) -c $*.c

.s.o:
	$(CC) -c $*.s
 
all: $(LIB)
 
include Make.lst
 
$(SLIB): $(OBJECTS)
	$(AR) $(ARFLAGS) $(SLIB) $?
	$(RAN) $(SLIB) > /dev/null
 
clean:
	@ $(RM) $(OBJECTS)
 
